// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:password@localhost:3306/iHero"
  
}

model User {
  id Int @id @default(autoincrement())

  email    String @unique
  password String

  name String
}